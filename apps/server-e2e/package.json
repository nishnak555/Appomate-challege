{
    "name": "@appomate-challenge/server-e2e",
    "version": "0.0.1",
    "private": true,
    "nx": {
        "implicitDependencies": [
            "@appomate-challenge/server"
        ],
        "targets": {
            "e2e": {
                "executor": "@nx/jest:jest",
                "outputs": [
                    "{projectRoot}/test-output/jest/coverage"
                ],
                "options": {
                    "jestConfig": "apps/server-e2e/jest.config.cts",
                    "passWithNoTests": true
                },
                "dependsOn": [
                    "@appomate-challenge/server:build",
                    "@appomate-challenge/server:serve"
                ]
            }
        }
    }
}
